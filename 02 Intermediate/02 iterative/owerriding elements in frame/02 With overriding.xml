<?xml version="1.0" encoding="UTF-8"?>
<testcase name="С переопределением" xmlns="http://www.jazzteam.org/Routines" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		  description="Заполняем поля с переопределением " 	
		  tags="Intermediate, итеративные, переопределение">
		  
	<import resource="ImportFile.xml" name="import"/>	  
		  
	<test coverage="100" name="01 Ivanov" 
		  description="В предыдущем тесткейсе в поле логин вводилось Ivanov, теперь NotIvanov " 
		  tags="field" >
		<navigate to="http://localhost:${jetty.port}/test/test/index.htm"/>
				
		<frame extends="import:logPass">
			<field ref="login" id="login" textToType="NotIvanov"/>
		</frame>		
	</test>
	
	<test coverage="100" name="02 Petrov" 
		  description="В предыдущем тесткейсе в поле логин вводилось Petrov, теперь NotPetrov " 
		  tags="field" >
		<navigate to="http://localhost:${jetty.port}/test/test/index.htm"/>
		
		<frame extends="import:logPass">
			<field ref="login" id="login" textToType="NotPetrov"/>
		</frame>
	</test>
	
	<test coverage="100" name="03 Sidorov" 
		  description="В предыдущем тесткейсе в поле логин вводилось Sidorov, теперь NotSidorov " 
		  tags="field, ref, frame" >
		<navigate to="http://localhost:${jetty.port}/test/test/index.htm"/>
		
		<frame extends="import:logPass">
			<field ref="login" id="login" textToType="NotSidorov"/>
		</frame>
	</test>
</testcase>	