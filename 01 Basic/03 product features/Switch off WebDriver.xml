<?xml version="1.0" encoding="UTF-8"?>
<testcase name="Switch off webdriver" xmlns="http://www.jazzteam.org/Routines" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		  tags="Basic, product features, webDriver" description="Switch off webdriver" webDriverOn="false">
		  
	<test coverage="100" name="01 Equals" 
		  description="Compare two different variables" 
		  tags="assert, equals">
		<assert action="equals" first="10" second="10"/>
	</test>
	
	<test coverage="100" name="02 MoreThan" 
		  description="Check first value greater then second" 
		  tags="assert, moreThan">
		<assert action="moreThan" first="10" second="1"/>
	</test>
	
	<test coverage="100" name="03 VariableExists" 
		  description="Check variable exists" 
		  tags="assert, variableExists, variable">
		
		<variable ref="var" value="xml2selenium"/>  
		<assert action="variableExists" first="var"/>
	</test>	  
</testcase>	